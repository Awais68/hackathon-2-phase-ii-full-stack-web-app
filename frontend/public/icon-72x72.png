<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 100 100" width="100" height="100">
  <defs>
    <linearGradient id="grad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#00ffff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0080ff;stop-opacity:1" />
    </linearGradient>
    <filter id="glow">
      <feGaussianBlur stdDeviation="3" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Outer ring -->
  <circle cx="50" cy="50" r="45" fill="none" stroke="url(#grad)" stroke-width="3" filter="url(#glow)"/>
  
  <!-- Inner circle -->
  <circle cx="50" cy="50" r="35" fill="rgba(10,10,20,0.9)" stroke="#00ffff" stroke-width="1"/>
  
  <!-- MI Text -->
  <text x="50" y="58" text-anchor="middle" font-family="Arial Black, sans-serif" font-size="28" font-weight="bold" fill="#00ffff" filter="url(#glow)">MI</text>
  
  <!-- Target crosshair -->
  <line x1="50" y1="10" x2="50" y2="25" stroke="#00ffff" stroke-width="2" opacity="0.7"/>
  <line x1="50" y1="75" x2="50" y2="90" stroke="#00ffff" stroke-width="2" opacity="0.7"/>
  <line x1="10" y1="50" x2="25" y2="50" stroke="#00ffff" stroke-width="2" opacity="0.7"/>
  <line x1="75" y1="50" x2="90" y2="50" stroke="#00ffff" stroke-width="2" opacity="0.7"/>
</svg>
